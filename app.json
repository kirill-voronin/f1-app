{
  "expo": {
    "name": "Formula Info",
    "slug": "FInfo",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ee191f"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ee191f"
      },
      "package": "com.formula.info.app",
      "versionCode": 2
    },
    "web": {
      "favicon": "./assets/adaptiveIcon.png"
    },
    "extra": {
      "eas": {
        "projectId": "46c2711c-1bf3-4a07-8400-71f2a7d26ec7"
      }
    }
  },
  "plugins": [
    [
      "expo-notifications",
      {
        "icon": "./assets/notification-icon.png"
      }
    ]
  ]
}